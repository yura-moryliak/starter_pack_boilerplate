{
  "name": "server_nestjs",
  "version": "0.0.1",
  "scripts": {
    "nest:build": "nest build",
    "nest:format": "prettier --write \"src/**/*.ts\"",
    "nest:start": "nest start",
    "nest:start:dev": "nest start --watch",
    "nest:start:debug": "nest start --debug --watch",
    "nest:start:prod": "node dist/server_nestjs/main.js",
    "nest:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nest:test": "jest",
    "nest:test:watch": "jest --watch",
    "nest:test:cov": "jest --coverage",
    "nest:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "nest:test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:stop:all": "pm2 stop all",
    "pm2:restart:all": "pm2 restart all",
    "pm2:delete:all": "pm2 delete all",
    "pm2:logs:all": "pm2 logs"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^11.0.8",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.8",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.11",
    "bcryptjs": "^3.0.2",
    "cache-manager": "^6.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "express": "^5.0.1",
    "google-auth-library": "^9.15.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "jwt-decode": "^4.0.0",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pm2": "^5.4.3",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.21",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@fastify/static": "^8.1.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/ua-parser-js": "^0.7.39",
    "@types/web-push": "^3.6.4",
    "concurrently": "^9.1.2",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "rootDir": "src"
}
